{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "*": ["node_modules/*", "src/types/*"],
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@router/*": ["router/*"],
      "@views/*": ["views/*"],
      "@helpers/*": ["helpers/*"],
      "@types/*": ["types/*"],
      "@partials/*": ["partials/*"],
      "@stores/*": ["stores/*"],
      "@lib/*": ["lib/*"],
      "__CONSTANTS__/*": ["__CONSTANTS__/*"]
    }
  }
}
